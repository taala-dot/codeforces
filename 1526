#include <iostream>
using namespace std;

void solve() {
    long long x;
    cin >> x;
    while (x >= 0) {
        if (x % 11 == 0) {
            cout << "YES\n";
            return;
        }
        x -= 111;
    }
    cout << "NO\n";
}

int main() {
    freopen("/home/ares/CODE/qt-code/input.txt", "r",stdin);
    freopen("/home/ares/CODE/qt-code/output.txt", "w",stdout);

    int t; cin >> t;
    while (t--) solve();
    return 0;
}
